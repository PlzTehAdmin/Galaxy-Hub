--Galaxy Hub Admin

local Admin = loadstring(game:HttpGet("https://raw.githubusercontent.com/PlzTehAdmin/Galaxy-Hub/main/Galaxy%20Admin%20Source%20Code"))()
local Admin2 = Admin.load(getgenv().themes.dark, "Galaxy Hub", true) --(<theme>, <name>, <built-in commands>)
local theme = {
    cmdBar = rgb(13,13,13),
    cmdFont = Enum.Font.Code,
    cmdTextColor = rgb(255,255,255),
    cmdTextSize = 22,
    infoFrame1 = rgb(25,25,25),
    colorSequence =  ColorSequence.new({
        ColorSequenceKeypoint.new(0,rgb(171,171,171)),
        ColorSequenceKeypoint.new(1,rgb(255,255,255))
    }),
    infoFrame2 = rgb(13,13,13),
    infoText1Color = rgb(255,255,255),
    infoText1Font = Enum.Font.Gotham,
    infoText1TSize = 14,
    infoText2Color = rgb(255,255,255),
    infoText2TSize = 15,
    infoText2Font = Enum.Font.SourceSans,
    inputAwaitingImage = 'rbxassetid://2122740641',
    suggestionBackFrameColor = rgb(25,25,25),
    correctionFrame = rgb(25,25,25),
    suggestionTextColor = rgb(13,13,13),
    suggestionTextFont = Enum.Font.Gotham,
    suggestionTextXAlignment = Enum.TextXAlignment.Left,
    suggestionTextTSize = 16,
    scrollBackImageColor = rgb(13,13,13),
    suggestionFont = Enum.Font.Gotham,
    suggestionTextColor = rgb(255,255,255),
    suggestionXAlignment = Enum.TextXAlignment.Left
}

Admin2.setPrefix(",")

Admin2.addCommand({name = "cmds",desc = "Gives player all available commands in galaxy hub",callback = function(v)
    local m = Admin2.getplayers(v)
    for i,v2 in pairs(m) do
        pcall(function()
            game:service('RunService').Stepped:connect(function()
                local prefix = ","
                local LPlayer = game.Players:FindFirstChild(v2)
                LPlayer.Chatted:Connect(function(msg)
                    msg = msg:lower()
                    if string.sub(msg,1,3) == "/e " then
                        msg = string.sub(msg,4)
                    end
                    if string.sub(msg,1,1) == prefix then
                        local cmd
                        local space = string.find(msg," ")
                        if space then
                            cmd = string.sub(msg,2,space-1)
                        else
                            cmd = string.sub(msg,2)
                        end

                        if cmd == "snap" then
                            local var = string.sub(msg,space+1)
                            local target = GetPlayer(var)
                            if game.Players.LocalPlayer == target then
                                pcall(function()
                                    local punchevent = game.Players.LocalPlayer.Backpack.suitControl.Assets.Events.punchHit
                                    for i,v in pairs(game.Players:GetChildren()) do
                                        punchevent:FireServer(target.Character.HumanoidRootPart, math.huge)
                                    end
                                end)
                            end
                        end
                        if cmd == "fling" then
                            local var = string.sub(msg,space+1)
                            local target = GetPlayer(var)
                            if game.Players.LocalPlayer == target then
                                pcall(function()
                                    local punchevent = game.Players.LocalPlayer.Backpack.suitControl.Assets.Events.punchHit
                                    local dmg = -(0/0)
                                    for i,v in pairs(game.Players:GetChildren()) do
                                        punchevent:FireServer(target.Character.HumanoidRootPart, dmg)
                                    local punchevent = game.Players.LocalPlayer.Backpack.suitControl.Assets.Events.punchHit
                                    local dmg = 999999999999999999999999999999999999
                                    for i,v in pairs(game.Players:GetChildren()) do
                                        punchevent:FireServer(target.Character.HumanoidRootPart, dmg)
                                    end
                                    end
                                end)
                            end
                        end
                        if cmd == "work" then
                            print("Work?...")
                        end
                    end
                end)
            end)

        function GetPlayer(String)
            local plr
            local strl = String:lower()
                for i, v in pairs(game:GetService("Players"):GetPlayers()) do
                    if v.Name:lower():sub(1, #String) == String:lower() then
                        plr = v
                    end
                end
                if String == "me" then
                        plr = game.Players.LocalPlayer
                    end
                if String == "" or String == " " then
                plr = nil
                end
            return plr
            end

        m.Text = v
        wait(1)
        m:Destroy()

        end)
    end
end})

Admin2.addCommand({name = "Kill",desc = "Only kills users with Galaxy",callback = function(v)
    local m = Admin2.getplayers(v)
    for i,v2 in pairs(m) do
        print("You don't have perms to kill other Galaxy users...")
    end

    m.Text = v
    wait(1)
    m:Destroy()
end})

Admin2.addCommand({name = "Ghcheck",desc = "Checks all users using Galaxy Hub",callback = function(v)
    local m = Admin2.getplayers(v)
    for i,v2 in pairs(m) do
        print("You do not have perms to check other users...")
    end

    m.Text = v
    wait(1)
    m:Destroy()
end})

Admin2.addCommand({name = "Shutdown",desc = "Kicks player using galaxy out of the server",callback = function(v)
    local m = Admin2.getplayers(v)
    for i,v2 in pairs(m) do
        print("You do not have perms to shutdown other users...")
    end

    m.Text = v
    wait(1)
    m:Destroy()
end})

Admin2.addCommand({name = "Harmless",desc = "Player looses ability to attack",callback = function(v)
    local m = Admin2.getplayers(v)
    for i,v2 in pairs(m) do
        pcall(function()
            local punchevent = game.Players.LocalPlayer.Backpack.suitControl.Assets.Events.punchHit
                for i,v in pairs(game.Players:GetChildren()) do
                    punchevent:FireServer(v2.Character.LeftHand, 0.8)
                    punchevent:FireServer(v2.Character.RightHand, 0.8)
                    punchevent:FireServer(v2.Character.UpperTorso, 1.8)
                end
            end)
        end

    m.Text = v
    wait(1)
    m:Destroy()
end})

Admin2.addCommand({name = "Loopharmless",desc = "Similar to loopkill except makes the player fly",callback = function(v)
    local m = Admin2.getplayers(v)
    for i,v2 in pairs(m) do
        pcall(function()
            local punchevent = game.Players.LocalPlayer.Backpack.suitControl.Assets.Events.punchHit
            game:service('RunService').Stepped:connect(function()
                for i,v in pairs(game.Players:GetChildren()) do
                    punchevent:FireServer(v2.Character.LeftHand, 0.5)
                    punchevent:FireServer(v2.Character.RightHand, 0.5)
                    punchevent:FireServer(v2.Character.UpperTorso, 1.8)
                end
            end)
            end)
        end

    m.Text = v
    wait(1)
    m:Destroy()
end})

Admin2.addCommand({name = "Snap",desc = "Kills player",callback = function(v)
    local m = Admin2.getplayers(v)
    for i,v2 in pairs(m) do
        pcall(function()
            local punchevent = game.Players.LocalPlayer.Backpack.suitControl.Assets.Events.punchHit
            local vector = Vector3.new(0, 0, 0) 
            local dmg = math.huge
                for i,v in pairs(game.Players:GetChildren()) do
                    punchevent:FireServer(v2.Character.HumanoidRootPart, dmg)
                end
            end)
        end

    m.Text = v
    wait(1)
    m:Destroy()
end})

Admin2.addCommand({name = "Loopkill",desc = "Loopkills player",callback = function(v)
    local m = Admin2.getplayers(v)
    for i,v2 in pairs(m) do
        pcall(function()
            local punchevent = game.Players.LocalPlayer.Backpack.suitControl.Assets.Events.punchHit
            local vector = Vector3.new(0, 0, 0) 
            local dmg = math.huge
            game:service('RunService').Stepped:connect(function()
                for i,v in pairs(game.Players:GetChildren()) do
                    punchevent:FireServer(v2.Character.HumanoidRootPart, dmg)
                end
            end)
        end)
    end

    m.Text = v
    wait(1)
    m:Destroy()
end})

Admin2.addCommand({name = "God",desc = "Gives god to player",callback = function(v)
    local m = Admin2.getplayers(v)
    for i,v2 in pairs(m) do
        pcall(function()
    local punchevent = game.Players.LocalPlayer.Backpack.suitControl.Assets.Events.punchHit
        local vector = Vector3.new(0, 0, 0) 
        local dmg = -(1/0)
                for i,v in pairs(game.Players:GetChildren()) do
                    punchevent:FireServer(v2.Character.HumanoidRootPart, dmg)
                end
            end)
        end

    m.Text = v
    wait(1)
    m:Destroy()
end})

