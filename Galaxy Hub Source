            local d
            d = game:service('RunService').Stepped:connect(function()
                if not value then
                    return d:Disconnect()
                end
        local hrp = game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart")
        local val = 0
            if hrp:FindFirstChild("FakeBPVelocity") then
                hrp.FakeBPVelocity.MaxForce = Vector3.new(val, val, val)
            end
            if hrp:FindFirstChild("BodyAngularVelocity") then
                hrp.BodyAngularVelocity.MaxTorque = Vector3.new(val, val, val)
            end
        end)
        game.Players.LocalPlayer.Character.Humanoid.Changed:Connect(function()
            pcall(function()
                if lpc.Humanoid.Sit == true then
                    lpc.Humanoid.Sit = false
                else
                    return
                end
            end)
        end)
        end
    end})

