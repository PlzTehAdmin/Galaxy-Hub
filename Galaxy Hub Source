                return
            else
            game:service('RunService').Stepped:connect(function(s)
        pcall(function()
        for _, v in next, game.Players:GetPlayers() do
                            if v.Backpack:FindFirstChild("suitControl") then
                                v.Backpack.suitControl.Assets.Events.punchHit:FireServer(game.Players[asdw].Character.HumanoidRootPart, math.huge)
                            end
                            end
                        end)
                    end)
                end
            end)
            wait(3)
        end})

