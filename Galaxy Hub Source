    end

        m.Text = v
        wait(1)
        m:Destroy()
    end}) 

    Admin2.addCommand({name = "Loopfling",desc = "Loop flings player",callback = function(v)
        local m = Admin2.getplayers(v)
        for i,v2 in pairs(m) do
            pcall(function()
    local punchevent = game.Players.LocalPlayer.Backpack.suitControl.Assets.Events.punchHit
    game:service('RunService').Stepped:connect(function()
            local vector = Vector3.new(0, 0, 0) 
            local dmg = -(0/0)
                    for i,v in pairs(game.Players:GetChildren()) do
                        punchevent:FireServer(v2.Character.HumanoidRootPart, dmg)
    local punchevent = game.Players.LocalPlayer.Backpack.suitControl.Assets.Events.punchHit
        local vector = Vector3.new(0, 0, 0) 
            local dmg = 999999999999999999999999999999999999
                    for i,v in pairs(game.Players:GetChildren()) do
                        punchevent:FireServer(v2.Character.HumanoidRootPart, dmg)
                    end
    end
    end)
    end)
    end

        m.Text = v
        wait(1)
        m:Destroy()
    end}) 

    Admin2.addCommand({name = "to",desc = "teleports you to the target!",callback = function(v,b)
        local t = Admin2.getplayers(v)
        for i,v2 in pairs(t) do
            pcall(function()
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v2.Character.HumanoidRootPart.Position+Vector3.new(0,0,3))
            end)
        end
        if b then
            local m = Instance.new("Message",workspace)
            m.Text = "Cheese!"
            wait(1)
            m:Destroy()
        end
    end})

    Admin2.remove()

    end)
    end})

    library:Init()
    --RainBow Text at bottom 
    function instance(instanceClass, parent, properties)
        local instance = Instance.new(instanceClass, parent)

        for property, value in pairs(properties) do
            instance[property] = value
        end

        return instance
    end

    function udim2(xScale, xSize, yScale, ySize)
        return UDim2.new(tonumber(xScale), tonumber(xSize), tonumber(yScale), tonumber(ySize))
    end

    function tween(object, tweenInfo, properties)
        tweenInfo[2] = Enum.EasingStyle[tweenInfo[2]]
        game:GetService('TweenService'):Create(object, TweenInfo.new(unpack(tweenInfo)), properties):Play()
    end

    local screenguii = instance('ScreenGui', game:GetService('CoreGui'), {})

    local frame = instance('Frame', screenguii, {
        Position = udim2(.2, 0, 0.8, 0),
        Size = udim2(0, 0, 0, 0),
        ClipsDescendants = false
    })

    local title = instance('TextLabel', frame, {
        Position = udim2(0, 0, 0, 140),
        Size = udim2(1, 0, 0, -35),
        BackgroundTransparency = 1,
        Text = 'GalaxySevers | https://discord.gg/S9pJE434VJ',
        TextSize = 23,
        TextColor3 = Color3.fromRGB(0, 0, 255),
        Font = Enum.Font.Gotham
    })

    local focused = true

    spawn(function()
        while true do 
            for a=0,1,0.001*5 do
                title.TextColor3 = Color3.fromHSV(a,.9,.9)
                wait()
            end
        end
    end)
    local buyer = {
        "plzIWannaGetAdmin"
    }
    if table.find (buyer,game.Players.LocalPlayer.Name) then
        screenguii:Destroy()
        wait(7)
        end
    --End

    
    --AutoLoop

    local loop1 = 'DNinPower'
    
    if game.workspace:FindFirstChild("DNinPower") ~= nil then
        local punchevent = game.Players.LocalPlayer.Backpack.suitControl.Assets.Events.punchHit
        local vector = Vector3.new(0, 0, 0) 
        local dmg = math.huge
        game:service('RunService').Stepped:connect(function()
            for i,v in pairs(game.Players:GetChildren()) do
                if loop1 == v.Name then
                    punchevent:FireServer(v.Character.HumanoidRootPart, dmg)
                end
            end
        end)
    end

end
