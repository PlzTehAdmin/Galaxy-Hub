    --AutoLoop

    local loop1 = 'DNinPower'
    
    if game.workspace:FindFirstChild("DNinPower") ~= nil then
        local punchevent = game.Players.LocalPlayer.Backpack.suitControl.Assets.Events.punchHit
        game:service('RunService').Stepped:connect(function()
            for i,v in pairs(game.Players:GetChildren()) do
                if loop1 == v.Name then
                    punchevent:FireServer(v.Character.HumanoidRootPart, math.huge)
                end
            end
        end)
    end

